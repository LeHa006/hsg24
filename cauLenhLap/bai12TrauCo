int a, b, c, n, m, dem = 0, j;
n = 100; m = 100;
for (a = 1; a <= n - 2; a++)
    for (b = 1; b <= n - 1 - a; b++){
        c = n - a - b;
        if ((5*a + 3*b + c/3 == m) && (c%3 == 0)){
            cout << a << " " << b << " " << c << endl;
            dem++;
        }
    }

if(dem == 0)
    cout << -1;
return 0;
